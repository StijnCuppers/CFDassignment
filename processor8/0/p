FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 46970;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 50000;
        value           uniform 46970;
    }
    Airfoil
    {
        type            zeroGradient;
    }
    Outlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    procBoundary8to7
    {
        type            processor;
        value           uniform 46970;
    }
    procBoundary8to9
    {
        type            processor;
        value           uniform 46970;
    }
}


