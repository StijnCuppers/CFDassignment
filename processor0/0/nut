FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.19926384;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            calculated;
        value           uniform 0.19926384;
    }
    Airfoil
    {
        type            nutkWallFunction;
        value           uniform 0.19926384;
    }
    Outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.19926384;
    }
    procBoundary0to7
    {
        type            processor;
        value           uniform 0.19926384;
    }
}


