FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "3";
    object      wallShearStress;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    Airfoil
    {
        type            calculated;
        value           nonuniform List<vector> 0();
    }
    Outlet
    {
        type            calculated;
        value           nonuniform List<vector> 0();
    }
    procBoundary12to4
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary12to5
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary12to6
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary12to11
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary12to13
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


