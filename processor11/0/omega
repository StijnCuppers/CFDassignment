FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 150000;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            fixedValue;
        value           uniform 150000;
    }
    Airfoil
    {
        type            omegaWallFunction;
        value           uniform 150000;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    procBoundary11to3
    {
        type            processor;
        value           uniform 150000;
    }
    procBoundary11to4
    {
        type            processor;
        value           uniform 150000;
    }
    procBoundary11to10
    {
        type            processor;
        value           uniform 150000;
    }
    procBoundary11to12
    {
        type            processor;
        value           uniform 150000;
    }
}


