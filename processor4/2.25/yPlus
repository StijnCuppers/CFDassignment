FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "2.25";
    object      yPlus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            calculated;
        value           uniform 0;
    }
    Airfoil
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    Outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to11
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to12
    {
        type            processor;
        value           uniform 0;
    }
}


