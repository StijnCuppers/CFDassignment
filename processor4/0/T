FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 245.48;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            totalTemperature;
        gamma           1.4;
        T0              uniform 250;
        value           uniform 245.48;
    }
    Airfoil
    {
        type            zeroGradient;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 245.48;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 245.48;
    }
    procBoundary4to11
    {
        type            processor;
        value           uniform 245.48;
    }
}


