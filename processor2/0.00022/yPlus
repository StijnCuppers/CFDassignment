FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.00022";
    object      yPlus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    Airfoil
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    Outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    procBoundary2to8
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to10
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to18
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to26
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to32
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to34
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to40
    {
        type            processor;
        value           uniform 0;
    }
}


