FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.0002";
    object      Cz;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    Airfoil
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    Outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    procBoundary10to2
    {
        type            processor;
        value           uniform 0.5;
    }
    procBoundary10to14
    {
        type            processor;
        value           uniform 0.5;
    }
    procBoundary10to18
    {
        type            processor;
        value           uniform 0.5;
    }
    procBoundary10to26
    {
        type            processor;
        value           uniform 0.5;
    }
    procBoundary10to30
    {
        type            processor;
        value           uniform 0.5;
    }
    procBoundary10to42
    {
        type            processor;
        value           uniform 0.5;
    }
    procBoundary10to50
    {
        type            processor;
        value           uniform 0.5;
    }
}


