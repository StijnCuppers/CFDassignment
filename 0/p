FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 46970;

boundaryField
{
    Inlet 
    {
      type             totalPressure;
      p0              uniform 50000;
      value           uniform 50000;
    }

    Outlet
    {
      type              fixedValue;
      value             uniform 46970;
    }


    Side
    {
        type            empty;
    }

    Airfoil 
    {
        type            zeroGradient;
    }
}


