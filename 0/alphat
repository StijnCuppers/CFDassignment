FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0]; // Corresponds to kg/(m.s)

internalField   uniform 0;

boundaryField
{

    Inlet
    {
        type            calculated;
        value           $internalField;
    }

    Outlet
    {
        type            calculated;
        value           $internalField;
    }

    Airfoil
    {
        type            compressible::alphatJayatillekeWallFunction;
        value           $internalField;
    }

    Side
    {
        type            empty;
    }
}

// ************************************************************************* //