FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 12.0048615;

boundaryField
{

    Inlet
    {
        type        turbulentIntensityKineticEnergyInlet;
        intensity   0.03;           // 3% turbulence
        U               U;             // name of the velocity field (usually just "U")
        value           uniform 0.01;  // dummy, overwritten internally
    }

    Outlet
    {
    type            inletOutlet;
    inletValue      uniform 1.48208e-6;    // used only when flow is into domain
    }


    Airfoil
    {
        type            kqRWallFunction; 
        value           $internalField; 
    }

    Side
    {
        type            empty;
    }

     #includeEtc "caseDicts/setConstraintTypes"
}

// ************************************************************************* //